#!/usr/bin/env python3
"""
KegDisplayDB Web Interface Launcher

This script launches the KegDisplayDB web interface with appropriate defaults.
"""

import sys
import os
from KegDisplayDB.web.webinterface import start, parse_args

def main():
    # Parse arguments from command line
    args = parse_args()
    
    # Run the web interface
    return start(args)

if __name__ == "__main__":
    sys.exit(main()) 